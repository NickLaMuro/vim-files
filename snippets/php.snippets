#for
snippet for
	for ($${2:i} = 0; $$2 < ${1:count}; $$2${3:++}) {
		${4:// code...}
	}

#do
snippet do
	do {
		${2:// code... }
	} while (${1:/* condition */});"

#array
snippet array
	$${1:arrayName} = array('${2}' => ${3});${4}

